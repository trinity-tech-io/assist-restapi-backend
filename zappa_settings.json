{
    "prod": {
        "app_function": "app.application",
        "profile_name": "eb-cli",
        "project_name": "assist-restapi-",
        "runtime": "python3.7",
        "s3_bucket": "assist-restapi-zappa",
        "aws_region": "us-east-1",
        "events": [
            {
                "function": "app.cronjob.send_tx_to_did_sidechain",
                "expression": "rate(2 minutes)"
            },
            {
                "function": "app.cronjob.reset_didpublish_daily_limit",
                "expression": "cron(0 0 * * *)"
            }
        ],
        "keep_warm": false
    }
}
